{
  "extends": [
    "prettier-standard",
    "standard-react"
  ],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "jsx-quotes": [
      "error",
      "prefer-double"
    ],
    "react/prop-types": [1, {
      "ignore": ["children"]
    }]
  },
  "env": {
    "jest": true
  },
  "globals": {
    "jsdom": false
  }
}
